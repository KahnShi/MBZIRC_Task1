<?xml version="1.0"?>
<launch>

  ############## DJI SDK bringup #############################
  <node pkg="dji_sdk" type="dji_sdk_node" name="dji_sdk" output="screen">
    <!-- node parameters -->
    <param name="drone_version" type="string" value="M100"/>
    <param name="serial_name" type="string" value="/dev/ttyUSB0"/>
    <param name="baud_rate" type="int" value="230400"/>
    <param name="app_id" type="int" value="1031959"/>
    <param name="app_version" type="int" value="1"/>
    <param name="app_bundle_id" type="string" value="Welcome to use dji-sdk"/>
    <param name="enc_key" type="string" value="ed59ebb66de2a99221de11485dbc4a06095b70a82db07a313115e2273ae7b6c7"/>
    <param name="groundstation_enable" type="int" value="1"/>
  </node>

  ############## PointGrey #########################################
  <include file="$(find dji_sdk_demo_shi)/launch/pointgrey_camera.launch" />

  <!--
  ############## Guiadance #########################################
  <node pkg="guidance" type="guidanceNode" name="guidance" output="screen" />
  -->

</launch>

